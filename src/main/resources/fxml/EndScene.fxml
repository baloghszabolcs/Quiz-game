<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.bszab.quiz.controllers.EndController"
            prefHeight="400.0" prefWidth="700.0">
            <!--styleClass="Endcss">-->
    <stylesheets>
        <URL value="@/styles/endStyle.css" />
    </stylesheets>
    <children>
        <ImageView fx:id="imagegrats" fitHeight="300.0" fitWidth="460.0" layoutX="26.0" layoutY="90.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@/images/congrats.png" />
            </image>
        </ImageView>
        <Button id="exitButton" fx:id="exitButton" layoutX="415.0" layoutY="105.0" mnemonicParsing="false" onAction="#endGame" prefHeight="56.0" prefWidth="147.0" text="exit" />
        <Label id="textScoreLabel" fx:id="textScoreLabel" layoutX="137.0" layoutY="17.0" prefHeight="32.0" prefWidth="401.0" text="CONGRATULATIONS!!!" textOverrun="CLIP" />
        <Label id="scoreLabel" fx:id="scoreLabel" layoutX="27.0" layoutY="64.0" prefHeight="32.0" prefWidth="383.0" text="score" textOverrun="CLIP" />
        <Button id="againButton" fx:id="againButton" layoutX="417.0" layoutY="214.0" mnemonicParsing="false" onAction="#again" prefHeight="56.0" prefWidth="147.0" text="play again" />
    </children>

</AnchorPane>
